# Wait for the Code Server to start
echo "$(date): Waiting for matlab to open port ${port}..."

if wait_until_port_used "${host}:${port}" 300; then
  echo "$(date): Discovered matlab listening on port ${port}!"
else
  echo "$(date): Timed out waiting for matlab to open port ${port}!"
  clean_up 1
fi

sleep 2
